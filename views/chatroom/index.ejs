<h1>Chatrooms Home</h1>
<a href="chatroom/new">Create Chatroom</a>
<a href="chatroom/livechat">Access Chatroom</a>
<h2>Chatrooms</h2>
<p>Search Chatroom</p>
<form action="/chatroom" method="get">
  <label for="title">Title</label>
  <input type="text" name="title" id="title" value="<%= searchOptions.title %> ">
  <button type="submit">Search</button>
</form>
<br>
<% chatrooms.forEach(chatroom => { %>
  <div>
    <%= chatroom.title %> 
    <a href="/chatroom/<%= chatroom.title %>"> Join</a>
  </div>
<%  }); %> 
